<?xml version="1.0" encoding="UTF-8"?>
<!--suppress ALL -->
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="propVM" type="com.arunwichsapplication.app.modules.prop.data.viewmodel.PropVM"/>
    </data>

    <LinearLayout
        android:id="@+id/linearProp"
        style="@style/groupStyleyellow_50"
        android:layout_width="match_parent"
        android:layout_height="815dp"
        android:gravity="start"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearHead"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_47pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_marginEnd="@dimen/_47pxh"
            android:gravity="start"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageArrowleft"
                android:layout_width="@dimen/_6pxh"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_marginBottom="@dimen/_5pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowleft"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_arrowleft" />

            <TextView
                android:id="@+id/txtPROPORTIONS"
                style="@style/txtIntersemibold20"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_72pxh"
                android:layout_weight="0.96"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{propVM.propModel.txtPROPORTIONS}"
                tools:ignore="TextSizeCheck"
                tools:text="@string/lbl_proportions" />
        </LinearLayout>

        <ImageView
            android:id="@+id/imageImageEight"
            imageRes="@{@drawable/img_image8}"
            android:layout_width="@dimen/_300pxh"
            android:layout_height="@dimen/_220pxh"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_30pxh"
            android:layout_marginEnd="@dimen/_23pxh"
            android:scaleType="fitXY"
            tools:ignore="ContentDescription,VisualLintBounds"
            tools:src="@drawable/img_image8" />

         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:padding="@dimen/_5pxh"
             android:layout_gravity="center"
             tools:ignore="VisualLintBounds">

             <EditText
                 android:id="@+id/linearWaistline"
                 android:layout_width="274dp"
                 android:layout_height="50dp"
                 android:layout_gravity="center_horizontal"
                 android:hint="Waistline"
                 android:textAlignment="center"
                 android:textColor="@color/black_900"
                 android:textSize="@dimen/_20pxh"
                 tools:ignore="VisualLintBounds,TextSizeCheck,DuplicateSpeakableTextCheck"></EditText>

             <TextView
                 android:id="@+id/txtInch"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_gravity="center_vertical"
                 android:text="@string/lbl_inch"
                 android:textSize="@dimen/_15pxh"
                 tools:ignore="TextSizeCheck" />


         </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_5pxh"
            android:layout_gravity="center"
            tools:ignore="VisualLintBounds">

            <EditText
                android:id="@+id/linearChest"
                android:layout_width="274dp"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:hint="Chest"
                android:textAlignment="center"
                android:textColor="@color/black_900"
                android:textSize="@dimen/_20pxh"
                tools:ignore="VisualLintBounds,TextSizeCheck"></EditText>

            <TextView
                android:id="@+id/txtInch2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/lbl_inch"
                android:textSize="@dimen/_15pxh"
                tools:ignore="TextSizeCheck" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_5pxh"
            android:layout_gravity="center"
            tools:ignore="VisualLintBounds">

            <EditText
                android:id="@+id/linearHip"
                android:layout_width="274dp"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:hint="Hip"
                android:textAlignment="center"
                android:textColor="@color/black_900"
                android:textSize="@dimen/_20pxh"
                tools:ignore="VisualLintBounds,TextSizeCheck"></EditText>

            <TextView
                android:id="@+id/txtInch3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/lbl_inch"
                android:textSize="@dimen/_15pxh"
                tools:ignore="TextSizeCheck" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_5pxh"
            android:layout_gravity="center"
            tools:ignore="VisualLintBounds">

            <EditText
                android:id="@+id/linearHeight"
                android:layout_width="274dp"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:hint="Height"
                android:textAlignment="center"
                android:textColor="@color/black_900"
                android:textSize="@dimen/_20pxh"
                tools:ignore="VisualLintBounds,TextSizeCheck"></EditText>

            <TextView
                android:id="@+id/txtcm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/lbl_cm"
                android:textSize="@dimen/_15pxh"
                tools:ignore="TextSizeCheck" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_5pxh"
            android:layout_gravity="center"
            tools:ignore="VisualLintBounds">

            <EditText
                android:id="@+id/linearWeight"
                android:layout_width="274dp"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:hint="Weight"
                android:textAlignment="center"
                android:textColor="@color/black_900"
                android:textSize="@dimen/_20pxh"
                tools:ignore="VisualLintBounds,TextSizeCheck"></EditText>

            <TextView
                android:id="@+id/txtInch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="@string/lbl_cm"
                android:textSize="@dimen/_15pxh"
                tools:ignore="TextSizeCheck" />


        </LinearLayout>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSuccess"
            style="@style/btnSolidRounded"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_2pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginBottom="@dimen/_20pxv"
            android:ellipsize="end"
            android:gravity="center"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_9pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingBottom="@dimen/_9pxv"
            android:singleLine="true"
            android:text="@string/lbl_success"
            android:textAllCaps="false"
            tools:ignore="TextSizeCheck,VisualLintButtonSize,VisualLintBounds"
            tools:text="@string/lbl_success" />
    </LinearLayout>
</layout>
